{
  "name": "prickly-pear",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "bun": "1"
  },
  "scripts": {
    "ci": "bun run fix && bun run typecheck && bun run typecheck:deno && bun run lint && bun run knip && bun run build && bun run test",
    "build": "bun run build.ts",
    "dev": "bun run --hot src/index.tsx",
    "lint": "eslint . && prettier --check .",
    "test": "vitest run",
    "clean": "rm -rf dist node_modules",
    "fix": "eslint --fix --quiet . && prettier --log-level=silent --write .",
    "typecheck": "tsc -b --noEmit",
    "typecheck:deno": "deno check supabase/functions/**/*.ts --quiet",
    "gen": "supabase gen types --project-id vgddrhyjttyrathqhefb > src/integrations/supabase/types.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org gwiz --project pricklypear ./dist && sentry-cli sourcemaps upload --org gwiz --project pricklypear ./dist",
    "prepare": "husky || true"
  },
