{
  "name": "prickly-pear",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "bun": "1"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,mdx}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "react-email"
  ],
  "prettier": "@dexaai/config/prettier",
  "packageManager": "bun@1.1.42",
  "scripts": {
    "ci": "bun run typecheck && bun run typecheck:deno && bun run lint && bun run knip && bun run build && bun run test",
    "build": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "dev:emails": "cd react-email && bun run dev",
    "lint": "eslint . && prettier --check .",
    "test": "bun run test:unittest && bun run test:deno",
    "test:unittest": "vitest run",
    "test:deno": "deno test --allow-env --allow-read --allow-net supabase/functions/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "clean:base": "rm -rf dist node_modules",
    "clean:emails": "cd react-email && rm -rf node_modules",
    "clean": "bun run clean:base && bun run clean:emails",
    "fix": "eslint --fix --quiet . && prettier --log-level=silent --write .",
    "typecheck": "tsc -b --noEmit",
    "typecheck:deno": "deno check --import-map=supabase/functions/deno.json --quiet supabase/functions/**/*.ts",
    "gen": "bun run gen:supabase && bun run gen:emails",
    "gen:supabase": "supabase gen types --project-id vgddrhyjttyrathqhefb > src/integrations/supabase/types.ts",
    "gen:emails": "bun run react-email/sync-emails-to-templates.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:status": "supabase status",
    "db:reset": "supabase db reset",
    "db:diff": "supabase db diff",
    "db:pull": "supabase db pull",
    "db:push": "supabase db push",
    "db:migration:new": "supabase migration new",
    "db:migration:up": "supabase migration up",
    "db:migration:list": "supabase migration list",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org gwiz --project pricklypear ./dist && sentry-cli sourcemaps upload --org gwiz --project pricklypear ./dist",
    "prepare": "husky || true",
    "prod:deploy": "supabase db push && supabase functions deploy && vercel deploy --prod",
    "prod:psql": "psql \"sslmode=verify-full sslrootcert=./supabase.crt host=aws-0-us-east-2.pooler.supabase.com dbname=postgres user=postgres.vgddrhyjttyrathqhefb\""
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-accordion": "1.2.12",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-aspect-ratio": "1.1.8",
    "@radix-ui/react-avatar": "1.1.11",
    "@radix-ui/react-checkbox": "1.3.3",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-context-menu": "2.2.16",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-hover-card": "1.1.15",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-menubar": "1.1.16",
    "@radix-ui/react-navigation-menu": "1.2.14",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-progress": "1.1.8",
    "@radix-ui/react-radio-group": "1.3.8",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.8",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@radix-ui/react-toggle": "1.1.10",
    "@radix-ui/react-tooltip": "1.2.8",
    "@sentry/cli": "3.0.1",
    "@sentry/react": "10.32.1",
    "@supabase/supabase-js": "2.89.0",
    "@tanstack/react-query": "5.90.12",
    "@vercel/analytics": "1.6.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "4.1.0",
    "i18next": "25.7.3",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "lucide-react": "0.562.0",
    "react": "19.2.3",
    "react-big-calendar": "1.19.4",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.3",
    "react-dropzone": "14.3.8",
    "react-hook-form": "7.68.0",
    "react-i18next": "16.5.0",
    "react-markdown": "10.1.0",
    "react-router-dom": "7.11.0",
    "remark-gfm": "4.0.1",
    "sonner": "2.0.7",
    "tailwind-merge": "3.4.0",
    "tw-animate-css": "1.4.0",
    "uuid": "13.0.0",
    "vaul": "1.1.2",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@dexaai/config": "0.0.10",
    "@eslint/js": "9.39.2",
    "@playwright/test": "1.57.0",
    "@react-email/render": "1.1.2",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/bun": "1.3.5",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-big-calendar": "1.16.3",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/ui": "4.0.16",
    "autoprefixer": "10.4.23",
    "baseline-browser-mapping": "2.9.11",
    "deno": "2.6.2",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jsdom": "27.3.0",
    "knip": "5.75.2",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.0",
    "vite": "7.3.0",
    "vitest": "4.0.16"
  },
  "overrides": {
    "baseline-browser-mapping": "2.9.11"
  }
}
